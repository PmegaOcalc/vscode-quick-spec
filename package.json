{
  "name": "vscode-create-spec",
  "displayName": "vscode-create-spec",
  "description": "Easy create a spec file for the opened filed",
  "version": "0.0.1",
  "publisher": "raulmelo",
  "engines": {
    "vscode": "^1.31.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raulfdm/vscode-create-spec"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.crevscateSpec"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createSpec",
        "title": "Create Spec File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./ --skipLibCheck",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
